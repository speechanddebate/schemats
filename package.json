{
	"name": "schemats",
	"type": "module",
	"version": "0.1.0",
	"description": "Schemats Frontend Client for Tabroom.com",
	"private": true,
	"author": "Chris Palmer <palmer@tabroom.com> (https://speechanddebate.org)",
	"license": "RPL 1.5",
	"keywords": [
		"speech",
		"schemats",
		"debate",
		"congress",
		"tabulation",
		"forensics",
		"tabroom",
		"speechanddebate",
		"NSDA"
	],
	"bugs": {
		"url": "https://github.com/speechanddebate/tabroom/issues"
	},
	"scripts": {
		"production": "vite build --mode production && node -r dotenv/config build/index.js",
		"staging": "vite build --mode staging",
		"build": "vite build --mode production",
		"dev": "vite dev",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "eslint .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@stylistic/eslint-plugin-js": "^2.13.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.27.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tanstack/svelte-query-devtools": "^5.66",
		"@tsconfig/svelte": "^5.0.4",
		"@types/node": "^22.10.5",
		"@typescript-eslint/parser": "^8.22.0",
		"@vitest/eslint-plugin": "^1.1.25",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.17.0",
		"eslint-import-resolver-typescript": "^3.7.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-svelte": "^2.36.0",
		"flowbite-svelte": "^0.48.1",
		"flowbite-svelte-icons": "^2.0.2",
		"globals": "^15.0.0",
		"husky": "^9.0.11",
		"svelte": "^5.38.0",
		"svelte-animated-icons": "^0.7.0",
		"svelte-check": "^4.0.0",
		"svelte-eslint-parser": "^0.43.0",
		"svelte-preprocess": "^6.0.3",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.0.0",
		"vite": "^6.2.0",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@faker-js/faker": "^9.5.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tanstack/svelte-query": "^5.80.6",
		"ag-grid-community": "^33.2.4",
		"tailwindcss": "^3.4.17"
	}
